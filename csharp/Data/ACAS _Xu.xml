<Environment name="ACAS_Xu_Environment" type="simulation">
    <Agents>
        <Agent id="1" type="Aircraft">
            <Attribute>
                <Name value="Ownship_Speed"/>
                <DataType value="float"/>
                <Value value="300"/>
                <Mutable value="true"/>
                <Constraint Min="10" Max="1100" oneOf="" roundOff=""/>
            </Attribute>
            <Attribute>
                <Name value="X"/>
                <DataType value="float"/>
                <Value value="0"/>
                <Mutable value="false"/>
                 <Constraint Min="-10000" Max="10000" oneOf="" roundOff="4"/>
            </Attribute>
            <Attribute>
                <Name value="Y"/>
                <DataType value="float"/>
                <Value value="0"/>
                <Mutable value="true"/>
                <Constraint Min="-10000" Max="10000" oneOf="" roundOff="4"/>
            </Attribute>
            <Attribute>
                <Name value="Theta"/>
                <DataType value="float"/>
                <Value value="1.5708"/>
                <Mutable value="true"/>
                <Constraint Min="-3.14" Max="3.14" oneOf="" roundOff="4"/>
            </Attribute>
        </Agent>
    </Agents>

    <Objects>
        <Object id="2" type="Intruder">
            <Attribute>
                <Name value="Intruder_Speed"/>
                <DataType value="float"/>
                <Value value="150"/>
                <Mutable value="true"/>
                <Constraint Min="10" Max="1100" oneOf="" roundOff="4"/>
            </Attribute>
            <Attribute>
                <Name value="X"/>
                <DataType value="float"/>
                <Value value="500"/>
                <Mutable value="true"/>
                <Constraint Min="-10000" Max="10000" oneOf="" roundOff="4"/>
            </Attribute>
            <Attribute>
                <Name value="Y"/>
                <DataType value="float"/>
                <Value value="500"/>
                <Mutable value="true"/>
                <Constraint Min="-10000" Max="10000" oneOf="" roundOff="4"/>
            </Attribute>
            <Attribute>
                <Name value="Auto_Theta"/>
                <DataType value="float"/>
                <Value value="0.7854"/>
                <Mutable value="true"/>
                <Constraint Min="-3.14" Max="3.14" oneOf="" roundOff="4"/>
            </Attribute>
        </Object>
    </Objects>

    <Attribute>
        <Name value="Timestep_Count"/>
        <DataType value="int"/>
        <Value value="150"/>
         <Mutable value="true"/>
        <Constraint Min="50" Max="10000" oneOf="" roundOff=""/>
    </Attribute>
</Environment>