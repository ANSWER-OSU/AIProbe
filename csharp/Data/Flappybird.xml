<Environment name="ComplexEnvironment" type="simulation">
  <Agents>
    <Agent id="1" type="Robot">
      <Attribute>
        <Name value="X"/>
        <Description value=""/>
        <DataType value="int"/>
        <Value value="2"/>
        <Mutable value="true"/>
        <Constraint Min="0" Max="300" oneOf="" roundOff=""/>
      </Attribute>
      <Attribute>
        <Name value="Y"/>
        <Description value=""/>
        <DataType value="int"/>
        <Value value="2"/>
        <Mutable value="true"/>
        <Constraint Min="0" Max="500" oneOf="" roundOff=""/>
      </Attribute>
    </Agent>
  </Agents>
  <Objects>
    <Object id="1" type="Pipe">
      <Attribute>
        <Name value="start_gap"/>
        <Description value="The initial distance between pipes and the bird's start position."/>
        <DataType value="int"/>
        <Value value="10"/>
        <Mutable value="true"/>
        <Constraint Min="0" Max="50" oneOf="" roundOff=""/>
      </Attribute>
      <Attribute>
        <Name value="pipe_gap"/>
        <Description value="The gap size between the top and bottom pipes."/>
        <DataType value="int"/>
        <Value value="10"/>
        <Mutable value="true"/>
        <Constraint Min="1" Max="100" oneOf="" roundOff=""/>
      </Attribute>
      <Attribute>
        <Name value="offset"/>
        <Description value="The random offset applied to pipe heights."/>
        <DataType value="int"/>
        <Value value="1"/>
        <Mutable value="true"/>
        <Constraint Min="0" Max="10" oneOf="" roundOff=""/>
      </Attribute>
    </Object>
  </Objects>
  <Attribute>
    <Name value="Gravity"/>
    <Description value="The width of the boundary"/>
    <DataType value="int"/>
    <Value value="10"/>
    <Mutable value="false"/>
    <Constraint Min="1" Max="10" oneOf="" roundOff=""/>
  </Attribute>
  <Attribute>
    <Name value="Speed"/>
    <Description value="The Speed of the environment moving left ."/>
    <DataType value="int"/>
    <Value value="10"/>
    <Mutable value="false"/>
    <Constraint Min="1" Max="10" oneOf="" roundOff=""/>
  </Attribute>

</Environment>


       