<Environment name="ComplexEnvironment" type="simulation">
  <Agents>
    <Agent id="1" type="Bird">
      <Attribute>
        <Name value="X"/>
        <DataType value="int"/>
        <Value value="200"/>
        <Mutable value="true"/>
        <Constraint Min="0" Max="113"/>
      </Attribute>
      <Attribute>
        <Name value="Y"/>
        <DataType value="int"/>
        <Value value="300"/>
        <Mutable value="true"/>
        <Constraint Min="0" Max="400"/>
      </Attribute>
      <Attribute>
        <Name value="FLAP_POWER"/>
        <DataType value="float"/>
        <Value value="9.0"/>
        <Mutable value="true"/>
        <Constraint Min="1" Max="15" roundOff="4"/>
      </Attribute>
      <Attribute>
        <Name value="MAX_DROP_SPEED"/>
        <DataType value="float"/>
        <Value value="10.0"/>
        <Mutable value="true"/>
        <Constraint Min="0" Max="10" roundOff="4"/>
      </Attribute>
    </Agent>
  </Agents>
  <Objects>
    <Object id="1" type="Pipe">
      <Attribute>
        <Name value="pipe_gap"/>
        <DataType value="int"/>
        <Value value="100"/>
        <Mutable value="true"/>
        <Constraint Min="20" Max="256"/>
      </Attribute>
    </Object>
  </Objects>
  <Attribute>
    <Name value="Gravity"/>
    <DataType value="float"/>
    <Value value="6.0"/>
    <Mutable value="false"/>
    <Constraint Min="1" Max="20" roundOff="4"/>
  </Attribute>
  <Attribute>
    <Name value="Timestep_Count"/>
    <DataType value="int"/>
    <Value value="150"/>
    <Mutable value="true"/>
    <Constraint Min="50" Max="10000"/>
  </Attribute>
  <Attribute>
    <Name value="scale"/>
    <DataType value="float"/>
    <Value value="1.0"/>
    <Mutable value="true"/>
    <Constraint Min="0.1" Max="2.0" roundOff="4"/>
  </Attribute>
</Environment>