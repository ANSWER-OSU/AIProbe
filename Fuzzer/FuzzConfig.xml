<FuzzerConfig>
    <!-- General Fuzzer Settings -->
    <Settings>
        <Timeout>5</Timeout> <!-- Timeout for each test case in min -->
        <LogFilePath>Result/Minigrid/FourRoom</LogFilePath>
        <Enviroment>four_room</Enviroment>
        <EnviromentConfig>A:\Github repos\Answer\AIProbe\Minigrid\Config.xml</EnviromentConfig>
    </Settings>

    <!-- Numeric Seed Inputs for the Fuzzer -->
    <SeedList>
        <Seed>11</Seed>
        <Seed>56</Seed>
        <Seed>32</Seed>
        <Seed>88</Seed>
        <Seed>41</Seed>
        <Seed>29</Seed>
        <Seed>78</Seed>
        <Seed>93</Seed>
        <Seed>100</Seed>
        <Seed>200</Seed>
        <!-- Add more seeds as needed -->
    </SeedList>
    <!-- Mutator Settings -->
    <Mutators>
        <Mutator>
            <Name>Insert</Name>
            <Enabled>true</Enabled>
            <Probability>0.5</Probability> <!-- Probability of applying this mutator -->
        </Mutator>
        <Mutator>
            <Name>Replace</Name>1
            <Enabled>true</Enabled>
            <Probability>0.45</Probability>
        </Mutator>
         <Mutator>
            <Name>Delete</Name>
            <Enabled>true</Enabled>
            <Probability>0.05</Probability>
        </Mutator>
    </Mutators>
</FuzzerConfig>
